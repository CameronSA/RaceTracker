<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <add key="DataMiningApp" value="RaceTrackerConsole"/>
    <!--Expected headers in data file-->
    <add key="DataHeaders" value="Going,Surface,Race Type,Age,Rated,Prize,Distance,Racetrack,Date,Time,Position,Draw,Horse Name,Horse Age,Horse Weight,ISP,Expectation"/> 
    <!--Date ranges (yyyy/MM/dd) for common analyses. If UseFullDataSet is set to 'true', date range is ignored and the full range of dates in the available data is used-->
    <add key="MinDate" value="2020/01/01"/>
    <add key="MaxDate" value="2020/01/01"/>
    <add key="UseFullDataSet" value="true"/>
    <add key="MaxNumberRaceCourses" value="10"/>
  </appSettings>
</configuration>